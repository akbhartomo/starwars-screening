{
  "name": "Blanja-Test",
  "version": "0.1.0",
  "description": "My own base project for React",
  "private": true,
  "dependencies": {
    "fetch": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/assets/sass -o src/assets/css",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/assets/sass -o src/assets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2"
  }
}
